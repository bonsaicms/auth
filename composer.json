{
    "name": "bonsaicms/auth",
    "type": "bonsaicms-plugin",
    "description": "Bonsai CMS backend auth package",
    "keywords": [
        "Bonsai CMS",
        "auth"
    ],
    "license": "MIT",
    "require": {
        "laravel/framework": "^8.0",
        "laravel/fortify": "^1.6",
        "laravel/sanctum": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "BonsaiCms\\": "src/BonsaiCms/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BonsaiCms\\Providers\\AuthServiceProvider"
            ]
        }
    }
}
